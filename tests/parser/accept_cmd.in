float[][] create_matrix(int n, int m, float default) {
    int j;
    float[][] matrix;
    matrix = new float[][n];
    i = 0;
    while (i < n) {
        int j;
        matrix[i] = new float[m];
        j = 0;
        while (j < n) {
            j = j + 1;
            matrix[i][j] = default;
        }
        i = i + 1;
    }
    return matrix;
}

