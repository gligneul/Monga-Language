int m_strlen(char[] str) {
    int len;
    len = 0;
    while (str[len] != 0)
        len = len + 1;
    return len;
}

char[] m_strcpycat(char[] a, char[] b) {
    int i, j;
    char[] out;
    out = new char[m_strlen(a) + m_strlen(b)];
    i = 0; j = 0;
    while (a[i] != 0) {
        out[j] = a[i];
        i = i + 1;
        j = j + 1;
    }
    i = 0;
    while (b[i] != 0) {
        out[j] = b[i];
        i = i + 1;
        j = j + 1;
    }
    out[j] = 0;
    return out;
}
