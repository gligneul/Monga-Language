void testIntegers() {
    print 2 + 2, "\n";
    print 5 - 3, "\n";
    print -(10 * 5) / 2, "\n";
}

void testFloats() {
    print 3 + 5.1 * 2, "\n";
    print 9 / 4.0 * 3.3, "\n";
    print 18 / 6. / 2015, "\n";
}

int main() {
    testIntegers();
    testFloats();
    return 0;
}

