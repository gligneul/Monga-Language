#include <mongaio.mngh>

int fat(int n) {
    if (n == 0) {
        return 1;
    } else {
        return n * fat(n - 1);
    }
}

int main() {
    int i;
    i = 0;
    while (i < 10) {
        printInt(fat(i));
        i = i + 1;
    }
    return 0;
}
